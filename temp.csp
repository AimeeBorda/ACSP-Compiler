transparent normal 


P = normal((let R = l?id -> (map(l,id) /\ R) 
 within (SKIP/\ R)[|{| l|}|] l!0 -> SKIP) \ {| l|})
Q = normal((let R = l?id -> (map(l,id) /\ R) 
 within (SKIP/\ R)[|{| l|}|] l!1 -> SKIP) \ {| l|})

assert P [T= Q

channel l : {0..1}

map = \ chName,id @ if chName == l and id == 0 then SKIP 
else if chName == l and id == 1 then STOP 
else SKIP
