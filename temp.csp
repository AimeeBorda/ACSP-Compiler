transparent normal 


D = normal(((let R = l?id -> (map(l,id) /\ R) 
 within (SKIP/\ R) |||let R = m?id -> (map(m,id) /\ R) 
 within (SKIP/\ R))[|{| l,m,n|}|] ( l!0 -> SKIP ||| n!0 -> SKIP)) \ {| l,m,n|})
assert D :[deadlock free [F]]

channel l : {0..0}
channel n : {0..0}

map = \ chName,id @ if chName == l and id == 0 then SKIP 
else if chName == n and id == 0 then SKIP 
else SKIP
